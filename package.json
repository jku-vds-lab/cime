{
  "name": "cime",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://github.com/jku-vds-lab/cime",
  "module": "./lib/export.js",
  "types": "./lib/export.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jku-vds-lab/cime.git"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "@mui/styles": "^5.0.2",
    "@types/d3": "^5.7.2",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.36",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.9",
    "d3v5": "npm:d3@5.16.0",
    "lodash": "~4.17.20",
    "node-sass": "^6.0.1",
    "projection-space-explorer": "git+https://github.com/jku-vds-lab/projection-space-explorer.git#develop",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loader-spinner": "^4.0.0",
    "react-promise-tracker": "^2.1.0",
    "react-redux": "^7.2.6",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "typescript": "^4.4.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "windows-start": "react-scripts start",
    "start": "NODE_OPTIONS=--max-old-space-size=8192 PORT=8080 react-scripts start",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "NODE_OPTIONS=--max-old-space-size=8192 npm run build",
    "deploy": "NODE_OPTIONS=--max-old-space-size=8192 gh-pages -d build",
    "precompile:lib": "rm -rf ./lib",
    "compile:lib": "tsc --build ./tsconfig.lib.json",
    "copy-files:lib": "copyfiles -u 1 \"src/**/*.scss\" \"src/**/*.svg\" lib/",
    "postcompile:lib": "npm run copy-files:lib"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "gh-pages": "^3.2.3"
  }
}
